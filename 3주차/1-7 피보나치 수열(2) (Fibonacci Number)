#include <stdio.h>
#include <stdlib.h>

//배열을 미리 생성하여 각 index에 계산한 값을 저장해서 효율적으로 바꾼 방법

int fib2(int arr[], int n){
    for(int i = 2 ; i < n ; i++)
        arr[i] = arr[i-1] + arr[i-2];
    
    return arr[n-1];
}



int main(int argc, const char * argv[]) {
    int n;
    int result;
    scanf("%d", &n);
        
    if(n < 1)
        return 1;
    
    int f[n];
    f[0]= 1;
    f[1]= 1;
    
    
    
    if(n <= 2){
        result = 1;
    }
    else{
        result = fib2(f, n);
    }
    
    for(int i = 0 ; i < n ; i++)
        printf("%d ", f[i]);
    printf("\nYour finded number is '%d'\n", result);

    
    return 0;
}
